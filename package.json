{
  "name": "object-crumble",
  "description": "Creative ways to cook your object",
  "keywords": [
    "object",
    "property-based testing",
    "end-to-end testing",
    "unit testing",
    "testing",
    "genetic programming"
  ],
  "version": "0.3.0",
  "author": {
    "name": "Olivier Huin",
    "url": "https://github.com/olih"
  },
  "license": "MIT",
  "homepage": "https://github.com/flarebyte/object-crumble",
  "repository": {
    "type": "git",
    "url": "https://github.com/flarebyte/object-crumble.git"
  },
  "bugs": "https://github.com/flarebyte/object-crumble/issues",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/src/index.mjs",
      "default": "./dist/src/index.mjs",
      "types": "./dist/src"
    },
    "./package.json": {
      "default": "./package.json"
    }
  },
  "files": [
    "dist/src",
    "src"
  ],
  "bin": {},
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "lint": "baldrick lint check",
    "lint:fix": "baldrick lint fix",
    "lint:ci": "baldrick lint ci",
    "test": "baldrick test check",
    "test:fix": "baldrick test fix",
    "test:cov": "baldrick test cov",
    "test:ci": "baldrick test ci",
    "reset": "rm -rf dist; rm -rf report",
    "prebuild": "yarn reset",
    "build": "tsc --outDir dist",
    "doc": "npx typedoc --json report/doc.json --pretty src/index.ts && npx baldrick-doc-ts typedoc --json-source report/doc.json",
    "ready": "yarn lint && yarn test:cov && yarn outdated",
    "github": "gh repo edit --delete-branch-on-merge --enable-squash-merge",
    "norm": "npx baldrick-ts generate -f lib -ga 'flarebyte' -ch 'Flarebyte.com' -cy 2021 -l MIT -b object-crumble",
    "norm:g": "baldrick-ts generate -f lib -ga 'flarebyte' -ch 'Flarebyte.com' -cy 2021 -l MIT -b object-crumble",
    "md": "yarn remark .",
    "md:fix": "yarn remark . --output",
    "md:help": "yarn remark --help",
    "md:f2": "npx prettier *.md --write"
  },
  "dependencies": {},
  "devDependencies": {
    "baldrick-dev-ts": "^0.8.5",
    "remark-cli": "^10.0.1",
    "remark-preset-lint-consistent": "^5.1.1",
    "remark-preset-lint-markdown-style-guide": "^5.1.2",
    "remark-preset-lint-recommended": "^6.1.2",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {}
}
