{
  "extends": "baldrick-tsconfig-es2020",
  "compilerOptions": {
    "module": "node16",
    "moduleResolution": "node16",
    "moduleDetection": "force",
    "target": "ES2020",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": false,
    "diagnostics": true,
    "extendedDiagnostics": true
  },
  "ts-node": {
    "transpileOnly": true,
    "files": true,
    "compilerOptions": {}
  }
}